  </servlet>
  <servlet-mapping>
    <servlet-name>Myservlet</servlet-name>
    <url-pattern>/myname</url-pattern>
  </servlet-mapping>
  
  
  
  
  <servlet>
    <description></description>
    <display-name>MyBookServlet</display-name>
    <servlet-name>MyBookServlet</servlet-name>
    <servlet-class>books.MyBookServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>MyBookServlet</servlet-name>
    <url-pattern>/ajinkyaservlet</url-pattern>
  </servlet-mapping>
  
</web-app>